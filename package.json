{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "react app using custom ssr",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"webpack/**/*.ts\"",
    "check-types": "tsc --noEmit",
    "start:webpack": "webpack serve --mode=development",
    "start:server": "nodemon index.js --watch dist/server.js",
    "start:dev": "npm-run-all --print-label --parallel start:*",
    "start:prod": "node index.js"
  },
  "keywords": ["react", "webpack", "express", "swc", "typescript"],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
